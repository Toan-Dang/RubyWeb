

<main>
        <%= stylesheet_link_tag "Home_productlist" %>
         <%= javascript_pack_tag 'productlist' %>
     <div class="container" style="justify-content: center">
        <div class="mb-3">
            <div class="d-inline">
                <a class="btn" id="Apple" asp-controller="Products" asp-action="Category" asp-route-id="16"><img src="https://cdn.tgdd.vn/Brand/1/logo-macbook-149x40.png" width="90" /></a>
                <a class="btn" id="Asus" asp-controller="Products" asp-action="Category" asp-route-id="17"><img src="https://cdn.tgdd.vn/Brand/1/logo-asus-149x40.png" width="90" /></a>
                <a class="btn" id="Acer" asp-controller="Products" asp-action="Category" asp-route-id="18"><img src="https://cdn.tgdd.vn/Brand/1/logo-acer-149x40.png" width="90" /></a>
                <a class="btn" id="Dell" asp-controller="Products" asp-action="Category" asp-route-id="19"><img src="https://cdn.tgdd.vn/Brand/1/logo-dell-149x40.png" width="90" /></a>
                <a class="btn" id="Lenovo" asp-controller="Products" asp-action="Category" asp-route-id="20"><img src="https://cdn.tgdd.vn/Brand/1/logo-lenovo-149x40.png" width="90" /></a>
                <a class="btn" id="HP" asp-controller="Products" asp-action="Category" asp-route-id="21"><img src="https://cdn.tgdd.vn/Brand/1/logo-hp-149x40-1.png" width="90" /></a>
                <a class="btn" id="MSI" asp-controller="Products" asp-action="Category" asp-route-id="22"><img src="https://cdn.tgdd.vn/Brand/1/logo-msi-149x40.png" width="90" /></a>
           </div>
        </div>
        </div>

     <div class="highlight">
          <% @Lap.each do |lap| %>
            <div class="displaying">
            <%= link_to product_path(lap) do %>
                    <div class="card">
                        <img class="card-img-top" src="<%= lap.Picture %>" alt="<%= lap.Color %>">
                        <div class="card-body">
                             <p class="rom-ram">
                                 <span id="ram">RAM
                                     <span id="ram-detail"><%= lap.Ram.Capacity%></span>&nbspGB&nbsp</span>
                                 <span id="rom">SSD <% if lap.Rom.Capacity == 1024 %>
                                  <span>1 TB</span>
                                  <% elsif lap.Rom.Capacity == 2048 %>
                          
                                        <span>1 TB</span>
                                    
                                    <% else %>
                                    
                                        <span><%= lap.Rom.Capacity %> GB</span>
                                    <% end %>
                                     </span>
                                </p>
                            <h6 class="card-title product-name"><%= lap.ProductName %></h6>
                            <h5 class="card-title pricesale m-0"><%= lap.UnitPrice %></h5>
                            <h6 class="card-title price text-muted mt-1"><%= lap.MSRP %></h6>
                            <div>
                                <p style="display:inline">
                                    <svg class="rating-checked bi bi-star-half" xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" viewBox="0 0 16 16">
                                        <path d="M3.612 15.443c-.386.198-.824-.149-.746-.592l.83-4.73L.173 6.765c-.329-.314-.158-.888.283-.95l4.898-.696L7.538.792c.197-.39.73-.39.927 0l2.184 4.327 4.898.696c.441.062.612.636.282.95l-3.522 3.356.83 4.73c.078.443-.36.79-.746.592L8 13.187l-4.389 2.256z" />
                                    </svg>
                                    <svg class="rating-checked bi bi-star-half" xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" viewBox="0 0 16 16">
                                        <path d="M3.612 15.443c-.386.198-.824-.149-.746-.592l.83-4.73L.173 6.765c-.329-.314-.158-.888.283-.95l4.898-.696L7.538.792c.197-.39.73-.39.927 0l2.184 4.327 4.898.696c.441.062.612.636.282.95l-3.522 3.356.83 4.73c.078.443-.36.79-.746.592L8 13.187l-4.389 2.256z" />
                                    </svg>
                                    <svg class="rating-checked bi bi-star-half" xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" viewBox="0 0 16 16">
                                        <path d="M3.612 15.443c-.386.198-.824-.149-.746-.592l.83-4.73L.173 6.765c-.329-.314-.158-.888.283-.95l4.898-.696L7.538.792c.197-.39.73-.39.927 0l2.184 4.327 4.898.696c.441.062.612.636.282.95l-3.522 3.356.83 4.73c.078.443-.36.79-.746.592L8 13.187l-4.389 2.256z" />
                                    </svg>
                                    <svg class="rating-checked bi bi-star-half" xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" viewBox="0 0 16 16">
                                        <path d="M3.612 15.443c-.386.198-.824-.149-.746-.592l.83-4.73L.173 6.765c-.329-.314-.158-.888.283-.95l4.898-.696L7.538.792c.197-.39.73-.39.927 0l2.184 4.327 4.898.696c.441.062.612.636.282.95l-3.522 3.356.83 4.73c.078.443-.36.79-.746.592L8 13.187l-4.389 2.256z" />
                                    </svg>
                                    <svg class="rating-checked bi bi-star-half" xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" viewBox="0 0 16 16">
                                        <path d="M5.354 5.119 7.538.792A.516.516 0 0 1 8 .5c.183 0 .366.097.465.292l2.184 4.327 4.898.696A.537.537 0 0 1 16 6.32a.548.548 0 0 1-.17.445l-3.523 3.356.83 4.73c.078.443-.36.79-.746.592L8 13.187l-4.389 2.256a.52.52 0 0 1-.146.05c-.342.06-.668-.254-.6-.642l.83-4.73L.173 6.765a.55.55 0 0 1-.172-.403.58.58 0 0 1 .085-.302.513.513 0 0 1 .37-.245l4.898-.696zM8 12.027a.5.5 0 0 1 .232.056l3.686 1.894-.694-3.957a.565.565 0 0 1 .162-.505l2.907-2.77-4.052-.576a.525.525 0 0 1-.393-.288L8.001 2.223 8 2.226v9.8z" />
                                    </svg>
                                </p>
                            </div>
                            <div class="information">
                            
                                <ul>
                                  
                                    <li id="screen"><p>Màn hình&nbsp<span id="inch"><%= lap.Screen.Size %></span>&nbspinch</p></li>
                                  
                                    <li id="chip"><p>Chip <span id="chip-name"><%= lap.Processor.Name %></span></p></li>
                                
                                    <li id="storage"><p><span id="ram"><%= lap.Ram.Capacity %></span>&nbspGB,&nbspSSD<span id="rom">
           
                                        <% if lap.Rom.Capacity == 1024 %>
                                        <span>1 TB</span>
                                        <% elsif lap.Rom.Capacity == 2048 %>
                                        <span>1 TB</span>
                                        <% else %>
                                           <span><%= lap.Rom.Capacity %> GB</span>
                                        <% end %>
                                    </span></p></li>  
                  
                                    <li id="battery">
                                        <p>Pin
                                            <span id="capacity">
                                            <% if lap.Battery.Capacity == 10 %>
                                         
                                                    <span>khoảng 10 tiếng</span>
                                              <% elsif lap.Battery.Capacity == 0 %>
                                                 <span> <%= lap.Battery.Charge + " W " + lap.Battery.Type  %></span>
                                                 <% else %>
                                                        <span><%= "lap.Battery.Capacity  lap.Battery.Type" %></span>
                                              <% end %>
                           
                                            </span>
                                        </p>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </div>
                <% end  %>
            </div>
        <% end %>
    </div>
    <script>
        $(document).ready(function () {
            $("#laptop").css({ "background-color": "white", "color": "#be1e2d" })
        });
    </script>
     <%= javascript_pack_tag "general" %>
</main>

